<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_type_benificaire_form" model="ir.ui.view">
        <field name="name">depense.type.benificaire.form</field>
        <field name="model">depense.type.benificaire</field>
        <field name="arch" type="xml">
            <form string="Type">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_type_benificaire_tree" model="ir.ui.view">
        <field name="name">depense.type.benificaire.tree</field>
        <field name="model">depense.type.benificaire</field>
        <field name="arch" type="xml">
            <tree string="Type" create="1" delete="1" editable="top">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_depense_type_benificaire" model="ir.actions.act_window">
        <field name="name">Type</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">depense.type.benificaire</field>
        <field name="view_mode">tree,form</field>
    </record>


</odoo>
