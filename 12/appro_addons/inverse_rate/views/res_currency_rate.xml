<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_res_currency_tree_inherited" model="ir.ui.view">
            <field name="name">res.currency.tree.view.inherited</field>
            <field name="model">res.currency.rate</field>
            <field name="inherit_id" ref="base.view_currency_rate_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='rate']" position="replace">
                    <field name="rate" digits="[12,12]"/>
                </xpath>
                <xpath expr="//field[@name='rate']" position="before">
                    <field name="taux_change_en_dinar"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>