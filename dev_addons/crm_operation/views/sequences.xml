<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data noupdate="1">

        <record id="seq_reservation" model="ir.sequence">
            <field name="name">number_reservation_sequence</field>
            <field name="code">crm.reservation</field>
            <field name="active">TRUE</field>
            <field name="prefix">RES/%(y)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True" />
        </record>

        <record id="seq_annulation" model="ir.sequence">
            <field name="name">number_annulation_sequence</field>
            <field name="code">crm.annulation</field>
