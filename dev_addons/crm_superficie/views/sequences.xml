<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data noupdate="1">

        <record id="seq_superficie" model="ir.sequence">
            <field name="name">number_superficie_sequence</field>
            <field name="code">crm.superficie</field>
            <field name="active">TRUE</field>
            <field name="prefix">SUP/%(y)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True"/>
        </record>
        <record model="ir.rule" id="superficie_rule">
            <field name="name">Superficie multi-company</field>
            <field name="model_id" ref="model_crm_superficie"/>
            <field name="domain_force">['|',('company_id', '=', False), ('company_id', 'in', company_ids)]</field>
        </record>

    </data>
</openerp>