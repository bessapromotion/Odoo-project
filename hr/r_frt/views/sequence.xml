<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record id="seq_frt" model="ir.sequence">
            <field name="name">number_frt_sequence</field>
            <field name="code">hr.frt</field>
            <field name="active">TRUE</field>
            <field name="prefix">FRT/%(range_year)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True" />
        </record>


        <record model="ir.rule" id="frt_rule">
            <field name="name">FRT multi-company</field>
            <field name="model_id" ref="model_hr_frt"/>
            <field name="domain_force">['|',('company_id', '=', False), ('company_id', 'in', company_ids)]</field>
        </record>
    </data>
</openerp>