<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data noupdate="1">

<!--       Typologie formation -->
      <record id="formation_typologie_pro" model="formation.typologie">
         <field name="name">Formation Professionnelle</field>
      </record>

      <record id="formation_typologie_phe" model="formation.typologie">
         <field name="name">Perfectionnement hors entreprise</field>
      </record>

      <record id="formation_typologie_pee" model="formation.typologie">
         <field name="name">Perfectionnement en entreprise</field>
      </record>

<!--       questions  -->
      <record id="formation_evaluation_question_1" model="formation.evaluation.question">
         <field name="name">Le contenu de la formation a t-il  répondu a vos besoins?</field>
          <field name="code" eval="1"/>
      </record>

      <record id="formation_evaluation_question_2" model="formation.evaluation.question">
         <field name="name">Comment jugez-vous l’organisation de la formation?</field>
          <field name="code" eval="2"/>
      </record>

      <record id="formation_evaluation_question_3" model="formation.evaluation.question">
         <field name="name">La durée a-t-elle été suffisante?</field>
          <field name="code" eval="3"/>
      </record>

      <record id="formation_evaluation_question_4" model="formation.evaluation.question">
         <field name="name">Que pensez-vous de la pédagogie utilisée par le formateur?</field>
          <field name="code" eval="4"/>
      </record>

      <record id="formation_evaluation_question_5" model="formation.evaluation.question">
         <field name="name">Que penser-vous de la documentation fournis?</field>
          <field name="code" eval="5"/>
      </record>

      <record id="formation_evaluation_question_6" model="formation.evaluation.question">
         <field name="name">Que pensez-vous des conditions matérielles de l’organisation?</field>
          <field name="code" eval="6"/>
      </record>

      <record id="formation_evaluation_question_7" model="formation.evaluation.question">
         <field name="name">La formation a-t-elle été bénéfique pour vous?</field>
          <field name="code" eval="7"/>
      </record>

      <record id="formation_evaluation_question_8" model="formation.evaluation.question">
         <field name="name">Comment vous sentez-vous après la formation?</field>
          <field name="code" eval="8"/>
      </record>

      <record id="formation_evaluation_question_9" model="formation.evaluation.question">
         <field name="name">Êtes-vous satisfait des possibilités d’utilisation des acquis dans vos fonctions?</field>
          <field name="code" eval="9"/>
      </record>


    </data>
</openerp>