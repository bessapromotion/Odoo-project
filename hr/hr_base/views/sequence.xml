<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data noupdate="1">

        <record id="seq_attestation_travail" model="ir.sequence">
            <field name="name">number_attestation_travail</field>
            <field name="code">hr.employee.at</field>
            <field name="active">TRUE</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True"/>
        </record>

        <record id="seq_certificat" model="ir.sequence">
            <field name="name">number_certificat_travail</field>
            <field name="code">hr.employee.ct</field>
            <field name="active">TRUE</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True"/>
        </record>
        <record id="seq_matricule" model="ir.sequence">
            <field name="name">number_matricule</field>
            <field name="code">hr.employee</field>
            <field name="active">TRUE</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True"/>
        </record>

        <record id="seq_fgp" model="ir.sequence">
            <field name="name">number_fgp</field>
            <field name="code">hr.employee.fgp</field>
            <field name="active">TRUE</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True"/>
        </record>

        <record id="seq_employee_historique" model="ir.sequence">
            <field name="name">number_employee_historique_sequence</field>
            <field name="code">hr.employee.historique</field>
            <field name="active">TRUE</field>
            <field name="prefix">%(range_year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True"/>
        </record>

        <record id="seq_registre" model="ir.sequence">
            <field name="name">number_registre_sequence</field>
            <field name="code">hr.registre</field>
            <field name="active">TRUE</field>
            <field name="prefix">DRH/%(range_year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True"/>
        </record>

    </data>
</openerp>
