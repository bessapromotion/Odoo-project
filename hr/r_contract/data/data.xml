<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data noupdate="1">

        <!--       Type contrat -->
        <record id="hr_contract_type_cdd" model="hr.contract.type">
            <field name="name">Contrat à durée déterminée</field>
            <field name="code">CDD</field>
        </record>

        <record id="hr_contract_type_cdi" model="hr.contract.type">
            <field name="name">Contrat à durée indéterminée</field>
            <field name="code">CDI</field>
        </record>

        <record id="ir_cron_data_trial_date_end" model="ir.cron">
            <field name="name">HR Contrat : Fin période d'essai</field>
            <field name="model_id" ref="model_hr_contract"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.get_trial_date_end()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>

    </data>
</openerp>