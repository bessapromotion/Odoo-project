        <record model="ir.rule" id="modele_rule">
            <field name="name">Modele multi-company</field>
            <field name="model_id" ref="model_hr_contract_modele"/>
            <field name="domain_force">['|',('company_id', '=', False), ('company_id', 'in', company_ids)]</field>
        </record>

        <record model="ir.rule" id="type_rule">
            <field name="name">Conract Type multi-company</field>
